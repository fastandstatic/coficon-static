<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>


<script>

function handleSubmit(event) {
    event.preventDefault();
    event.stopPropagation();
    const formok = validaForm();
    if (!formok) 
    return
    const data = new FormData(event.target);
    const value = Object.fromEntries(data.entries());
    const elements = event.target.elements;
    console.log(value);
    
    var obj = {};
    for (var i = 0; i < elements.length; i++) {
        var item = elements.item(i);

        if (item.type === 'checkbox') {
            if (!obj[item.name]) {
                obj[item.name] = [];
            }

            if (item.checked) {
                obj[item.name].push(item.value);
            }
        } else {
            obj[item.name] = item.value;
        }
    }

    // Convertir arrays a strings separados por coma
    for (var key in obj) {
        if (Array.isArray(obj[key])) {
            obj[key] = obj[key].join(', '); // Unir los valores con coma
        }
    }

    // Renombrar la propiedad 
    //obj['form_fields[beneficios]'] = obj['form_fields[beneficios][]'];
    //delete obj['form_fields[beneficios][]'];
 
    

    console.log(obj);

    fetch("https://hook.us1.make.com/j7j9mkw2lyxbvmrttzazdlzmg51gxman",
        {
            body: JSON.stringify(obj),
            headers: {
                Accept: 'application/json',
                'Content-Type': 'application/json',
            },
            method: "post",
        
    }) 
    .then((value) =>{
         
        console.log('todo ok');
        evento();
        Swal.fire({
            title: "Tu mensaje fue recibido, muy pronto te contactaremos",
            icon: "success",
            confirmButtonColor: "#133F77",
            confirmButtonText: "Cerrar",
        });
    })
    .catch((error) => {
        console.error('Error:', error);
    });
}
  
  

 function evento() {
    //Se cambia el nombre del evento que se necesita que se dispare en google y facebook
     var evento= "formulario-enviado-landing";
    //fbq("trackCustom", evento), 
    gtag("event", evento, {
        event_category: "Form", 
       event_label: evento 
    });
}

function validaForm() {
    event.preventDefault();
    var camposObligatorios = document.querySelectorAll('[required="required"]');
    for (var i = 0; i < camposObligatorios.length; i++) {
        if (camposObligatorios[i].value.trim() === '') {
            Swal.fire({
            title: "Por favor completa los campos obligatorios",
            icon: "error",
            confirmButtonColor: "#133F77",
            confirmButtonText: "Cerrar",
        });
            return false;
        }
    }
    
    return true;
}



function utm_llenar(){
        const valores = window.location.search;
        const urlParams = new URLSearchParams(valores);
        var term = urlParams.get('utm_term');
        var medium = urlParams.get('utm_medium');
        var campaign = urlParams.get('utm_campaign');
        var source = urlParams.get('utm_source');
        var content = urlParams.get('utm_content');
        document.getElementById("form-field-utm_term").value=term;
        document.getElementById("form-field-utm_medium").value=medium;
        document.getElementById("form-field-utm_campaign").value=campaign;
        document.getElementById("form-field-utm_source").value=source;
        document.getElementById("form-field-utm_content").value=content;
        //console.log("UTM LISTAS");
      }
 
 
    function addEvent() {
        // Se coloca ID del formulario que se configura en additional options en la edición del formulario de elementor
        // si existen más de un formulario en la web se debe cambiar la variable form agregando un numero al final
        utm_llenar();
        const form = document.querySelector('#form_coficon');
        console.log("form",form);
        if(!form){"No se encontro formulario con id"};
      form.addEventListener('submit', handleSubmit);
    }
  
  window.onload= addEvent();
 
document.addEventListener('DOMContentLoaded', function(){
        let formulario = document.getElementById('form_coficon');
        formulario.addEventListener('submit', function() {
          formulario.reset();
        });
    });

</script>	


